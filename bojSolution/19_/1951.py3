n=int(input())
if n>=1: r=0;i=1
if n>=10: r=1*9;i=2
if n>=100: r+=2*90;i=3
if n>=1000: r+=3*900;i=4
if n>=10000: r+=4*9000;i=5
if n>=100000: r+=5*90000;i=6
if n>=1000000: r+=6*900000;i=7
if n>=10000000: r+=7*9000000;i=8
if n>=100000000: r+=8*90000000;i=9
if n>=1000000000: r+=9*900000000;i=10
r+=(n-(10**(i-1)-1))*i
print(r%1234567)